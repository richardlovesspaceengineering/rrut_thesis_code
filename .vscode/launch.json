  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "cwd": "${fileDirname}"
    },
    {
        "name": "Python: Run Single Problem",
        "type": "python",
        "request": "launch",
        "program": "${file}",
        "args": ["MW11", "10", "1"],
        "console": "integratedTerminal"
    }
    
  ]
}
